{
  "name": "@galaxyops/design-system",
  "private": false,
  "version": "2.4.0",
  "module": "commonjs",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "bin": {
    "app": "bin/app.js"
  },
  "scripts": {
    "dev": "storybook dev -p 3000 -s public",
    "build": "tsc --build ./tsconfig-build.json && vite build",
    "build:storybook": "tsc --p ./tsconfig-build.json && storybook build",
    "build:all": "rush build -t .",
    "artifact": "create-artifact @galaxyops/design-system",
    "test": "jest --runInBand",
    "test:cov": "jest --coverage --runInBand",
    "watch": "tsc -w",
    "lint": "eslint --format visualstudio \"./src/**/*.tsx\" --fix && stylelint \"./src/**/*.scss\" --fix",
    "lint:ci": "eslint --format visualstudio \"./src/**/*.tsx\" --fix-dry-run && stylelint \"./src/**/*.scss\" ",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:diff": "cdk diff",
    "cdk:synth": "cdk synth",
    "cdk:hotswap": "cdk deploy --hotswap",
    "cdk:deploy": "storybook build && cdk deploy",
    "cdk:watch": "cdk watch",
    "cdk:destroy": "cdk destroy",
    "depcheck": "npx depcheck"
  },
  "peerDependencies": {
    "@emotion/react": "~11.11.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "~6.4.2",
    "@fortawesome/free-regular-svg-icons": "~6.4.2",
    "@fortawesome/free-solid-svg-icons": "~6.4.2",
    "@galaxyops/base-nodejs": "0.2.0",
    "@galaxyops/promare-gradients": "3.2.0",
    "@popperjs/core": "2.11.8",
    "bootstrap": "~5.3.2",
    "jsdom": "~23.0.1",
    "lodash": "4.17.21",
    "xss": "~1.0.14"
  },
  "devDependencies": {
    "@babel/core": "7.23.5",
    "@babel/preset-env": "7.23.5",
    "@babel/preset-react": "7.23.3",
    "@babel/preset-typescript": "7.23.3",
    "@emotion/react": "~11.11.1",
    "@emotion/styled": "11.11.0",
    "@fortawesome/react-fontawesome": "~0.2.0",
    "@galaxyops/character-sheet-contracts": "0.1.0",
    "@galaxyops/create-artifact": "4.3.0",
    "@galaxyops/eslint-config": "3.2.0",
    "@mui/lab": "~5.0.0-alpha.134",
    "@mui/material": "~5.12.2",
    "@mui/system": "~5.12.1",
    "@rollup/plugin-multi-entry": "~6.0.1",
    "@storybook/addon-a11y": "7.6.3",
    "@storybook/addon-actions": "7.6.3",
    "@storybook/addon-essentials": "7.6.3",
    "@storybook/addon-interactions": "7.6.3",
    "@storybook/addon-links": "7.6.3",
    "@storybook/addon-styling": "~1.3.7",
    "@storybook/builder-vite": "7.6.3",
    "@storybook/node-logger": "7.6.3",
    "@storybook/react": "7.6.3",
    "@storybook/react-vite": "7.6.3",
    "@storybook/testing-library": "0.2.2",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/dom": "9.3.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "14.5.1",
    "@types/jest": "29.5.12",
    "@types/lodash": "4.17.6",
    "@types/node": "20.11.25",
    "@types/react": "18.2.59",
    "@types/react-dom": "18.2.19",
    "@types/react-modal": "3.16.3",
    "@types/react-syntax-highlighter": "~15.5.10",
    "@types/styled-components": "~5.1.30",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "~6.16.0",
    "@typescript-eslint/parser": "~6.16.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "~10.4.16",
    "aws-cdk": "2.129.0",
    "aws-cdk-lib": "2.129.0",
    "axios": "^1.6.7",
    "constructs": "^10.2.70",
    "eslint": "8.57.0",
    "eslint-plugin-react-hooks": "~4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "glob": "^10.3.10",
    "identity-obj-proxy": "~3.0.0",
    "jest": "29.7.0",
    "react": "18.2.0",
    "react-day-picker": "8.9.1",
    "react-dom": "18.2.0",
    "react-google-charts": "~4.0.1",
    "react-intl": "6.5.5",
    "react-router-dom": "6.22.1",
    "react-syntax-highlighter": "~15.5.0",
    "sass": "^1.47.0",
    "storybook": "7.6.3",
    "stylelint": "~16.3.1",
    "stylelint-config-standard-scss": "~13.0.0",
    "stylelint-order": "~6.0.4",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "uuid": "~9.0.1",
    "vite": "^4.5.0",
    "vite-plugin-checker": "0.6.2",
    "vite-plugin-dts": "^3.6.3",
    "vite-plugin-lib-inject-css": "^1.3.0"
  },
  "watch": {
    "copy-files": {
      "patterns": [
        "src"
      ],
      "extensions": "css,scss",
      "runOnChangeOnly": true
    }
  },
  "author": "hxtree",
  "license": "MIT"
}
