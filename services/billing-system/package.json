{
  "name": "@galaxyops/billing-system",
  "module": "commonjs",
  "version": "2.2.0",
  "description": "Billing and payments system",
  "bin": {
    "app": "bin/app.js"
  },
  "scripts": {
    "artifact": "create-artifact @galaxyops/billing-system",
    "build": "tsc",
    "build:all": "nx run-many --target=build --all",
    "lint": "eslint --format visualstudio \"./stacks/**/*.ts\"  --fix",
    "lint:ci": "eslint --format visualstudio \"./stacks/**/*.ts\"  --fix-dry-run",
    "cdk": "cdk",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:synth": "cdk synth",
    "cdk:diff": "cdk diff",
    "cdk:deploy": "cdk deploy",
    "cdk:hotswap": "cdk deploy --hotswap",
    "cdk:watch": "cdk watch",
    "cdk:destroy": "cdk destroy",
    "depcheck": "npx depcheck"
  },
  "dependencies": {
    "@galaxyops/base-nodejs": "0.2.0",
    "@galaxyops/constructs": "2.2.0",
    "@galaxyops/validation-schemas": "workspace:*",
    "@nestjs/axios": "~2.0.0",
    "@nestjs/common": "9.4.0",
    "@nestjs/core": "9.2.1",
    "@nestjs/platform-express": "9.3.9",
    "@nestjs/terminus": "9.1.1",
    "@nestjs/testing": "9.2.1",
    "@vendia/serverless-express": "4.12.6",
    "aws-lambda": "1.0.7",
    "aws-serverless-express": "~3.4.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "eslint": "8.57.0",
    "express": "4.19.2",
    "node-cache": "5.1.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "source-map-support": "~0.5.21"
  },
  "devDependencies": {
    "@galaxyops/create-artifact": "4.3.0",
    "@galaxyops/create-bundle": "3.2.0",
    "@galaxyops/eslint-config": "3.2.0",
    "@types/aws-lambda": "~8.10.130",
    "@types/express": "~4.17.21",
    "@types/jest": "29.5.12",
    "@types/lodash": "4.17.6",
    "@types/node": "20.11.25",
    "@types/supertest": "~2.0.16",
    "@types/uuid": "~9.0.7",
    "aws-cdk": "2.129.0",
    "aws-cdk-lib": "2.129.0",
    "constructs": "^10.2.70",
    "esbuild": "~0.20.1",
    "fs": "~0.0.2",
    "jest": "29.7.0",
    "lodash": "4.17.21",
    "path": "~0.12.7",
    "supertest": "~6.3.4",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  }
}
