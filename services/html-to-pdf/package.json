{
  "name": "@cats-cradle/html-to-pdf",
  "module": "commonjs",
  "version": "3.1.0",
  "license": "MIT",
  "description": "Your Go-To Solution for PDF Generation from HTML.",
  "bugs": {
    "url": "https://github.com/hxtree/galaxyops/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hxtree/galaxyops.git"
  },
  "bin": {
    "app": "bin/app.js"
  },
  "scripts": {
    "artifact": "create-artifact @cats-cradle/html-to-pdf",
    "dev": "nest start --debug --watch",
    "build": "tsc",
    "build:all": "rush build -t .",
    "watch": "tsc -w",
    "test": "jest --runInBand",
    "test:cov": "jest --coverage --runInBand",
    "lint": "eslint --format visualstudio \"./src/**/*.ts\"  --fix",
    "lint:ci": "eslint --format visualstudio \"./src/**/*.ts\"  --fix-dry-run",
    "docs": "typedoc --entryPointStrategy expand ./src",
    "cdk": "cdk",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:synth": "cdk synth",
    "cdk:diff": "cdk diff",
    "cdk:deploy": "cdk deploy",
    "cdk:hotswap": "cdk deploy --hotswap",
    "cdk:watch": "cdk watch",
    "cdk:destroy": "cdk destroy",
    "depcheck": "npx depcheck"
  },
  "dependencies": {
    "@galaxyops/base-nodejs": "0.1.0",
    "@cats-cradle/constructs": "2.1.0",
    "@cats-cradle/faker-factory": "3.1.0",
    "@cats-cradle/nestjs-modules": "2.1.0",
    "@cats-cradle/validation-schemas": "2.1.0",
    "@nestjs/axios": "~2.0.0",
    "@nestjs/common": "9.4.0",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.2.1",
    "@nestjs/platform-express": "9.3.9",
    "@nestjs/swagger": "~7.1.17",
    "@nestjs/terminus": "9.1.1",
    "@nestjs/testing": "9.2.1",
    "@sparticuz/chromium": "112.0.0",
    "@sparticuz/chromium-min": "112.0.0",
    "@vendia/serverless-express": "4.12.6",
    "aws-lambda": "1.0.7",
    "aws-serverless-express": "~3.4.0",
    "axios": "^1.6.7",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "eslint": "8.57.0",
    "express": "4.19.2",
    "node-cache": "5.1.2",
    "puppeteer-core": "19.8.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "source-map-support": "~0.5.21",
    "uuid": "~9.0.1"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "3.433.0",
    "@cats-cradle/create-artifact": "2.0.7",
    "@cats-cradle/create-bundle": "1.0.12",
    "@galaxyops/eslint-config": "3.1.1",
    "@nestjs/cli": "^10.3.2",
    "@types/aws-lambda": "~8.10.130",
    "@types/express": "~4.17.21",
    "@types/jest": "29.5.12",
    "@types/lodash": "4.17.6",
    "@types/node": "20.11.25",
    "@types/supertest": "~2.0.16",
    "@types/uuid": "~9.0.7",
    "aws-cdk": "2.129.0",
    "aws-cdk-lib": "2.129.0",
    "aws-sdk-client-mock": "3.0.0",
    "axios": "^1.6.7",
    "constructs": "^10.2.70",
    "esbuild": "~0.20.1",
    "fs": "~0.0.2",
    "jest": "29.7.0",
    "lodash": "4.17.21",
    "path": "~0.12.7",
    "supertest": "~6.3.4",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  }
}
