{
  "name": "@galaxyops/email-service",
  "description": "A service for sending emails",
  "module": "commonjs",
  "version": "2.2.0",
  "scripts": {
    "dev": "nest start --debug --watch",
    "build": "tsc",
    "build:all": "rush build -t .",
    "artifact": "create-artifact @galaxyops/email-service",
    "watch": "tsc -w",
    "test": "jest --runInBand",
    "test:cov": "jest --coverage --runInBand",
    "lint": "eslint --format visualstudio \"./{stacks,src}/**/*.ts\"  --fix",
    "lint:ci": "eslint --format visualstudio \"./{stacks,src}/*.ts\"  --fix-dry-run",
    "docs": "typedoc --entryPointStrategy expand ./src",
    "cdk": "cdk",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:diff": "cdk diff",
    "cdk:synth": "cdk synth",
    "cdk:hotswap": "cdk deploy --hotswap",
    "cdk:deploy": "cdk deploy",
    "cdk:watch": "cdk watch",
    "cdk:destroy": "cdk destroy",
    "depcheck": "npx depcheck"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "~3.279.0",
    "@galaxyops/base-nodejs": "0.2.0",
    "@galaxyops/constructs": "2.2.0",
    "@galaxyops/messaging-schemas": "2.2.0",
    "@galaxyops/nestjs-modules": "2.2.0",
    "@galaxyops/validation-schemas": "2.2.0",
    "@nestjs/common": "9.4.0",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.2.1",
    "@nestjs/mongoose": "9.2.1",
    "@nestjs/platform-express": "9.3.9",
    "@nestjs/swagger": "~7.1.17",
    "@nestjs/terminus": "9.1.1",
    "@vendia/serverless-express": "4.12.6",
    "aws-lambda": "1.0.7",
    "aws-serverless-express": "~3.4.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "express": "4.19.2",
    "handlebars": "~4.7.8",
    "html-to-text": "~9.0.4",
    "lodash": "4.17.21",
    "mongoose": "6.11.3",
    "node-cache": "5.1.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "source-map-support": "~0.5.21",
    "uuid": "~9.0.1"
  },
  "devDependencies": {
    "@galaxyops/create-artifact": "4.3.0",
    "@galaxyops/create-bundle": "3.2.0",
    "@galaxyops/eslint-config": "3.2.0",
    "@galaxyops/faker-factory": "3.3.0",
    "@microsoft/tsdoc": "~0.13.2",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/testing": "9.2.1",
    "@shelf/jest-mongodb": "~4.2.0",
    "@types/aws-lambda": "~8.10.130",
    "@types/express": "~4.17.21",
    "@types/html-to-text": "~9.0.0",
    "@types/jest": "29.5.12",
    "@types/lodash": "4.17.6",
    "@types/node": "20.11.25",
    "@types/supertest": "~2.0.16",
    "@types/uuid": "~9.0.7",
    "aws-cdk": "2.129.0",
    "aws-cdk-lib": "2.129.0",
    "constructs": "^10.2.70",
    "esbuild": "~0.20.1",
    "eslint": "8.57.0",
    "fs": "~0.0.2",
    "jest": "29.7.0",
    "mongodb": "6.0.0",
    "path": "~0.12.7",
    "supertest": "~6.3.4",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  }
}
