{
  "name": "@galaxyops/api",
  "module": "commonjs",
  "description": "routes request to other endpoints",
  "version": "2.2.0",
  "bin": {
    "app": "bin/app.js"
  },
  "scripts": {
    "dev": "nest start --debug --watch",
    "artifact": "create-artifact @galaxyops/api",
    "build": "tsc",
    "build:all": "nx run-many --target=build --all --with-deps --tag=.",
    "watch": "tsc -w",
    "test": "jest --runInBand",
    "test:cov": "jest --coverage --runInBand",
    "lint": "eslint --format visualstudio \"./{stacks,src}/*.{ts,tsx}\"  --fix",
    "lint:ci": "eslint --format visualstudio \"./{stacks,src}/*.{ts,tsx}\"  --fix-dry-run",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:diff": "cdk diff",
    "cdk:synth": "cdk synth",
    "cdk:hotswap": "cdk deploy --hotswap",
    "cdk:deploy": "cdk deploy",
    "cdk:watch": "cdk watch",
    "cdk:destroy": "cdk destroy",
    "depcheck": "npx depcheck",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@galaxyops/base-nodejs": "0.2.0",
    "@galaxyops/constructs": "2.2.0",
    "@galaxyops/nestjs-modules": "2.2.0",
    "@galaxyops/validation-schemas": "workspace:*",
    "@nestjs/common": "9.4.0",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.2.1",
    "@nestjs/mongoose": "9.2.1",
    "@nestjs/platform-express": "9.3.9",
    "@nestjs/swagger": "~7.1.17",
    "@nestjs/terminus": "9.1.1",
    "@vendia/serverless-express": "4.12.6",
    "aws-lambda": "1.0.7",
    "aws-serverless-express": "~3.4.0",
    "axios": "^1.6.7",
    "class-transformer": "0.5.1",
    "express": "4.19.2",
    "node-cache": "5.1.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "source-map-support": "~0.5.21",
    "uuid": "~9.0.1"
  },
  "devDependencies": {
    "@galaxyops/base-nodejs": "workspace:*",
    "@galaxyops/constructs": "workspace:*",
    "@galaxyops/create-artifact": "4.3.0",
    "@galaxyops/create-bundle": "3.2.0",
    "@galaxyops/eslint-config": "3.2.0",
    "@nestjs/swagger": "~7.1.17",
    "@nestjs/testing": "9.2.1",
    "@types/aws-lambda": "~8.10.130",
    "@types/express": "~4.17.21",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.25",
    "@types/supertest": "~2.0.16",
    "@types/uuid": "~9.0.7",
    "aws-cdk": "2.129.0",
    "aws-cdk-lib": "2.129.0",
    "constructs": "^10.2.70",
    "esbuild": "~0.20.1",
    "eslint": "8.57.0",
    "fs": "~0.0.2",
    "jest": "29.7.0",
    "path": "~0.12.7",
    "supertest": "~6.3.4",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  }
}
