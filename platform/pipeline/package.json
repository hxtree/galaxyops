{
  "name": "@galaxyops/pipeline",
  "module": "commonjs",
  "version": "3.2.0",
  "description": "An API for simulating dice rolls and luck from dice notation",
  "bin": {
    "app": "bin/app.js"
  },
  "scripts": {
    "build": "tsc",
    "build:all": "rush build -t .",
    "watch": "tsc -w",
    "test": "jest --runInBand",
    "test:cov": "jest --coverage --runInBand",
    "cdk": "cdk",
    "hotswap": "cdk deploy --hotswap",
    "docs": "typedoc --entryPointStrategy expand ./src",
    "cdk:deploy": "cdk deploy",
    "cdk:destroy": "cdk destroy",
    "cdk:diff": "cdk diff",
    "cdk:synth": "cdk synth",
    "cdk:bootstrap": "cdk bootstrap",
    "depcheck": "npx depcheck"
  },
  "dependencies": {
    "@galaxyops/base-nodejs": "0.2.0",
    "@galaxyops/constructs": "2.2.0",
    "aws-lambda": "1.0.7",
    "eslint": "8.57.0"
  },
  "devDependencies": {
    "@galaxyops/eslint-config": "3.2.0",
    "@types/aws-lambda": "~8.10.130",
    "@types/jest": "29.5.12",
    "@types/lodash": "4.17.6",
    "@types/node": "20.11.25",
    "aws-cdk": "2.129.0",
    "aws-cdk-lib": "2.129.0",
    "constructs": "^10.2.70",
    "fs": "~0.0.2",
    "jest": "29.7.0",
    "lodash": "4.17.21",
    "path": "~0.12.7",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  }
}
